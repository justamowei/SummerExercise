<script type="text/javascript">//<![CDATA[
var swiperPic = new Swiper('.g_main .swiper-container', {
    watchSlidesProgress: true,
    watchSlidesVisibility: true,
    slidesPerView: 'auto', //寬度自動
    preloadImages: false,
    lazy: true,
    spaceBetween: 10, //間格10
    centeredSlides: true, //置中
//loop: true, //輪迴
    threshold: 50, //大於20才動
    pagination: {
        el: '.g_main .swiper-pagination',
        clickable: true
    },
    navigation: {
        nextEl: '.g_main .swiper-button-next',
        prevEl: '.g_main .swiper-button-prev'
    },
    on: {
        slideChangeTransitionStart: function () {
            $('.g_s .swiper-slide').eq(this.activeIndex).click();
        }
    }
});
var swiperPic2 = new Swiper('.g_s .swiper-container', {
    watchSlidesProgress: true,
    watchSlidesVisibility: true,
    preloadImages: false,
    lazy: true,
    setWrapperSize: true,
    slidesPerView: 'auto', //寬度自動
    threshold: 20, //大於20才動
    spaceBetween: 5
});
$('.g_s').find('.swiper-slide').click(function () {
    $(this).siblings('.active-nav').removeClass('active-nav').end().addClass('active-nav');
    $('.g_main .swiper-pagination-bullet').eq($(this).index()).click();
    var _aW = $('.g_s').width(),
        _gW = $('.g_s').find('.swiper-wrapper').width(),
        _gL = $('.g_s').find('.swiper-slide').length,
        _aI = $(this).index(),
        l_w = ((_aW - _gW + ((_aW - _gW) / _gL)) / _gL) * _aI;
    swiperPic2.setTransition(500);
    swiperPic2.setTranslate(_aW > _gW - 10 ? 0 : l_w, 0, 0);
});
var $pS = $('.point .swiper-container'),
    ps_L = $pS.find('.swiper-slide').length,
    pSwiper = new Swiper($pS, {
        navigation: {
            nextEl: $pS.parent().find('.swiper-button-next'),
            prevEl: $pS.parent().find('.swiper-button-prev')
        },
        spaceBetween: 15,
        slidesPerView: '5',
        preloadImages: false,
        lazy: true,
        loop: ps_L < 5 ? false : true,//如果子區塊少於5關閉迴圈
        paginationClickable: true,
        breakpoints: {
            970: {
                slidesPerView: 2,
                autoplay: 5000,
                autoplayDisableOnInteraction: false
            }
        }
    });

//]]>
</script>
